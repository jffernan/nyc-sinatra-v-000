<table>
  <th>Landmark Name</th>
  <th>Year Completed</th>
  <tr>
    <% @landmarks.each do |landmark| %>
      <td><a href="/landmarks/<%= landmark.id %>"><%= landmark.name %></a></td>
      <td align="center"><%= landmark.year_completed %></td>
  </tr>
  <% end %>
</table>
<br>
<a class="button" href="/landmarks/new">Create Landmark</a>
<a class="button" href="/figures">View Figures</a>
<a class="button" href="/figures/new">Create Figure</a>
<a class="button" href="/">HOME</a>
